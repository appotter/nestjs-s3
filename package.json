{
  "name": "@appotter/nestjs-s3",
  "version": "2.0.1",
  "description": "NestJS provider to integrates with AWS S3",
  "author": "Phitsanu Chuamuangphan <ap.phitsanu@gmail.com, contact@phitsanu.ch>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "*.md"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.json",
    "test": "jest --detectOpenHandles",
    "test:cov": "jest --detectOpenHandles --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appotter/nestjs-s3.git"
  },
  "keywords": [
    "nestjs",
    "aws-s3",
    "s3",
    "file-storage",
    "nodejs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/appotter/nestjs-s3/issues"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "aws-sdk": "^2.1204.0",
    "multer": "^1.4.5-lts.1",
    "rxjs": "^7.5.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/testing": "^9.0.11",
    "@types/jest": "^28.1.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0",
    "aws-sdk": "^2.1195.0",
    "multer": "^1.4.5-lts.1",
    "rxjs": "^7.5.6",
    "uuid": "^8.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
