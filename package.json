{
  "name": "@appotter/nestjs-s3",
  "version": "3.0.2",
  "description": "NestJS provider to integrates with AWS S3",
  "author": "Phitsanu Chuamuangphan <ap.phitsanu@gmail.com, contact@phitsanu.ch>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "*.md"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.json",
    "test": "jest --detectOpenHandles",
    "test:cov": "jest --detectOpenHandles --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appotter/nestjs-s3.git"
  },
  "keywords": [
    "nestjs",
    "aws-s3",
    "s3",
    "file-storage",
    "nodejs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/appotter/nestjs-s3/issues"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.502.0",
    "@aws-sdk/lib-storage": "^3.502.0",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/testing": "^10.3.1",
    "@smithy/util-stream": "^2.1.1",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "aws-sdk-client-mock": "^3.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0",
    "@aws-sdk/client-s3": "^3.502.0",
    "@aws-sdk/lib-storage": "^3.502.0",
    "uuid": "^9.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
