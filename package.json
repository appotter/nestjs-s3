{
  "name": "nestjs-s3",
  "version": "1.0.0",
  "description": "NestJS provider to integrates with AWS S3",
  "author": "Phitsanu Chuamuangphan <ap.phitsanu@gmail.com, contact@phitsanu.ch>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "*.md"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.json",
    "test": "jest"
  },
  "keywords": [
    "nestjs",
    "aws-s3",
    "s3",
    "file-storage",
    "nodejs"
  ],
  "dependencies": {
    "@nestjs/common": "^8.4.7",
    "aws-sdk": "^2.1195.0",
    "multer": "^1.4.5-lts.1",
    "rxjs": "^7.5.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.4.7",
    "@nestjs/testing": "^8.4.7",
    "@types/jest": "^28.1.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.0",
    "jest": "28.1.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "ts-jest": "28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.4.7",
    "aws-sdk": "^2.1195.0",
    "multer": "^1.4.5-lts.1",
    "rxjs": "^7.5.6",
    "uuid": "^8.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
